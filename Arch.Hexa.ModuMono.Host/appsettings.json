{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",

  "ServiceName": "Architecture.Pocs.ModularMonolith",

  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=CommonEntityService;Integrated Security=true"
  },

  "Cors": {
    "AllowedOrigins": [ "https://localhost:5001", "http://localhost:5000" ]
  },

  "CustomLogging": {
    "Console": {
      "Enabled": true,
      "LogLevel": 0
    },
    "File": {
      "Enabled": true,
      "LogLevel": 0,
      "PathFormat": "logs\\application-log.log"
    },
    "Seq": {
      "Enabled": true,
      "LogLevel": 0,
      "ServerUrl": "http://localhost:534"
    }
  },

  "Telemetry": {
    "ServerUrl": "http://localhost:4317"
  }


}
